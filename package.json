{
  "name": "aspectT",
  "version": "1.0.0",
  "description": "A seamless aspect-oriented extension to the Typescript™ programming language.Javascript platform compatible.Easy to learn and use.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && tsc && rollup -c",
    "docs": "typedoc --out docs src",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'test/**/*.{ts,tsx}'",
    "release": "./bin/release/release.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "AOP",
    "Aspect-Oriented Programming",
    "TypeScript",
    "JavaScript",
    "AspectJ",
    "Clean modularization",
    "Crosscutting concerns",
    "Crosscutting",
    "Error checking and handling",
    "Synchronization",
    "Context-sensitive behavior",
    "Performance optimization",
    "Monitoring and logging",
    "Debugging support",
    "Multi-object protocols"
  ],
  "author": "Aimé Biendo",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "rollup": "^4.24.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.11",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typescript": "^5.6.3",
    "winston": "^3.16.0"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.15",
    "inquirer": "^12.0.1",
    "reflect-metadata": "^0.2.2",
    "shelljs": "^0.8.5",
    "ts-morph": "^24.0.0"
  }
}
